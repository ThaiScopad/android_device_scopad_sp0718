<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

	<add-resource type="bool" name="config_unplugTurnsOnScreen">true</add-resource>
	<add-resource type="bool" name="config_allowAllRotations">true</add-resource>
	<add-resource type="bool" name="config_batterySdCardAccessibility">true</add-resource>
	<add-resource type="bool" name="config_enableLockScreenRotation">true</add-resource>
	<add-resource type="string" name="config_networkLocationProviderPackageName">com.google.android.location</add-resource>
	<add-resource type="string" name="config_geocodeProviderPackageName">com.google.android.location</add-resource>
	<add-resource type="bool" name="config_automatic_brightness_available">false</add-resource>
	<add-resource type="bool" name="config_voice_capable">false</add-resource>
	<add-resource type="bool" name="config_sms_capable">true</add-resource>
	<add-resource type="bool" name="config_screenOffAnimation">false</add-resource>
	<add-resource type="bool" name="config_showNavigationBar">true</add-resource>
	<add-resource type="bool" name="config_wifi_dual_band_support">false</add-resource>
	<add-resource type="bool" name="config_wifi_background_scan_support">true</add-resource>
	<add-resource type="bool" name="config_wifi_p2p_support">false</add-resource>
	<add-resource type="integer" name="config_screenBrightnessDim">30</add-resource>
	<add-resource type="integer" name="db_connection_pool_size">3</add-resource>
	<add-resource type="integer" name="config_cursorWindowSize">2048</add-resource>
	<add-resource type="integer" name="config_lowBatteryCloseWarningLevel">15</add-resource>
	<add-resource type="integer" name="config_lowBatteryWarningLevel">10</add-resource>

</resources>
